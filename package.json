{
  "name": "lol-addict",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "migrate": "db-migrate up",
    "watch": "npm run watch:server & npm run watch:bundle & npm run watch:sass",
    "productionify": "npm run prod:bundle && npm run prod:sass",
    "watch:server": "nodemon --config nodemon.server.json --exec node server.js",
    "watch:bundle": "watchify src/js/bundles/app_bundle.js -v -t babelify -g envify -p livereactload -o public/app.js",
    "watch:sass": "nodemon --config nodemon.sass.json --exec node-sass src/css/app.scss public/app.css ",
    "prod:bundle": "NODE_ENV=production browserify src/js/bundles/app_bundle.js -t babelify -g envify -g uglifyify > public/app.js",
    "prod:sass": "NODE_ENV=production node-sass src/css/app.sass public/app.css --output-style=compressed"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.22.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.7",
    "browserify": "^13.3.0",
    "busboy": "^0.2.12",
    "classnames": "^2.2.5",
    "connect-roles": "^3.1.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "envify": "^4.0.0",
    "express": "^4.14.0",
    "express-validator": "^2.18.0",
    "form-data": "^0.2.0",
    "hash-int": "^1.0.0",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^5.4.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.7",
    "mongoose": "^4.12.1",
    "morgan": "~1.6.1",
    "multer": "^1.1.0",
    "node-sass": "^4.3.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.2",
    "pug": "^2.0.0-beta6",
    "react": "^15.6.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.2",
    "react-redux": "^5.0.2",
    "react-retina-image": "^2.0.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-immutable": "^3.0.10",
    "request-ip": "^2.0.1",
    "reselect": "^2.5.4",
    "rollup": "^0.40.2",
    "rollup-plugin-babel": "^2.7.1",
    "serve-favicon": "~2.3.0",
